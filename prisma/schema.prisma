generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Admins {
  id       Int    @id @default(autoincrement())
  username String @db.VarChar(255)
  password String @db.VarChar(255)
}

model Users {
  id       Int    @id @default(autoincrement())
  username String @db.VarChar(255)
  email    String @unique
  password String @db.VarChar(255)
}
